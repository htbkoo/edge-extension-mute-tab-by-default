{
  "manifest_version": 3,
  "name": "Mute Tab By Default",
  "version": "0.12.0",
  "description": "A lightweight extension to mute all newly created tabs by default",
  "icons": {
    "16": "icons/16px-Mute_Icon.svg.png",
    "32": "icons/32px-Mute_Icon.svg.png",
    "64": "icons/64px-Mute_Icon.svg.png",
    "128": "icons/128px-Mute_Icon.svg.png",
    "256": "icons/256px-Mute_Icon.svg.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/16px-Mute_Icon.svg.png",
      "32": "icons/32px-Mute_Icon.svg.png",
      "64": "icons/64px-Mute_Icon.svg.png",
      "128": "icons/128px-Mute_Icon.svg.png",
      "256": "icons/256px-Mute_Icon.svg.png"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "author": "htbkoo"
}
